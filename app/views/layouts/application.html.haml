!!!
%html
  %head
    %title BlueNavy
    = stylesheet_link_tag "application", :media => "all"
    = javascript_include_tag "//js.pusher.com/1.12/pusher.min.js"
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body{class: body_dom_classes, data: body_data}
    - if user_signed_in?
      %nav#nav.navbar.navbar-static-top
        .navbar-inner
          .container
            = button_to "Log Out", session_url, method: :destroy, form: { class: "navbar-form" }
    .container
      = yield
